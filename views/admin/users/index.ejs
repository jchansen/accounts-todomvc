<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>View</th>
      </tr>
    </thead>
    <tbody>
      <% _.each(users, function (user, index) { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= user.username %></td>
          <td><a href="/users/<%= user.username %>">view</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>